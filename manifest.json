{
  "name": "Google Image shortcut and image-url extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "An extension that lets you find images and image-urls faster than going onto google images",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["js/angular/angular.min.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "http://google.com/",
    "https://ajax.googleapis.com/"
  ]
}
